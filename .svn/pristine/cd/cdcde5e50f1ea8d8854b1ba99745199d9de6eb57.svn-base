package com.chinasofti.pms.zmx.diaodu.entity;

public class DayPlan {
	private Integer id;
	private String date;//生产日期
	private Float rawcoalnum;//原煤数量
	private Float blindcoalnum;//无烟煤数量
	private Float browncoalnum;//褐煤数量
	private Float softcoalnum;//烟煤数量
	private Float bogheadcoalnum;//泥煤数量
	public DayPlan(Integer id, String date, Float rawcoalnum, Float blindcoalnum, Float browncoalnum, Float softcoalnum,
			Float bogheadcoalnum) {
		super();
		this.id = id;
		this.date = date;
		this.rawcoalnum = rawcoalnum;
		this.blindcoalnum = blindcoalnum;
		this.browncoalnum = browncoalnum;
		this.softcoalnum = softcoalnum;
		this.bogheadcoalnum = bogheadcoalnum;
	}
	public DayPlan(String date, Float rawcoalnum, Float blindcoalnum, Float browncoalnum, Float softcoalnum,
			Float bogheadcoalnum) {
		super();
		this.date = date;
		this.rawcoalnum = rawcoalnum;
		this.blindcoalnum = blindcoalnum;
		this.browncoalnum = browncoalnum;
		this.softcoalnum = softcoalnum;
		this.bogheadcoalnum = bogheadcoalnum;
	}
	public DayPlan() {
		super();
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public String getDate() {
		return date;
	}
	public void setDate(String date) {
		this.date = date;
	}
	public Float getRawcoalnum() {
		return rawcoalnum;
	}
	public void setRawcoalnum(Float rawcoalnum) {
		this.rawcoalnum = rawcoalnum;
	}
	public Float getBlindcoalnum() {
		return blindcoalnum;
	}
	public void setBlindcoalnum(Float blindcoalnum) {
		this.blindcoalnum = blindcoalnum;
	}
	public Float getBrowncoalnum() {
		return browncoalnum;
	}
	public void setBrowncoalnum(Float browncoalnum) {
		this.browncoalnum = browncoalnum;
	}
	public Float getSoftcoalnum() {
		return softcoalnum;
	}
	public void setSoftcoalnum(Float softcoalnum) {
		this.softcoalnum = softcoalnum;
	}
	public Float getBogheadcoalnum() {
		return bogheadcoalnum;
	}
	public void setBogheadcoalnum(Float bogheadcoalnum) {
		this.bogheadcoalnum = bogheadcoalnum;
	}
	@Override
	public String toString() {
		return "DayPlan [id=" + id + ", date=" + date + ", rawcoalnum=" + rawcoalnum + ", blindcoalnum=" + blindcoalnum
				+ ", browncoalnum=" + browncoalnum + ", softcoalnum=" + softcoalnum + ", bogheadcoalnum="
				+ bogheadcoalnum + "]";
	}
}
